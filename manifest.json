{
    "manifest_version": 3,
    "name": "Web Scraper",
    "version": "1.0",
    "description": "Extract innerHTML from web pages",
    "permissions": ["activeTab"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
      
        "128": "logo.png"
      }
    },
    "icons": {
        "128": "logo.png"
    },
    "host_permissions": [
      "http://localhost:8000/*"
    ]
  }
  